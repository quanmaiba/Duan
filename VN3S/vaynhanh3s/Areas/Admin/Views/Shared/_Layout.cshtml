<!DOCTYPE html>
<html>
<head>
    <base href="~/Admin/">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.PageTitle - VayNhanh3S</title>
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/bootstrap/dist/css/bootstrap.min.css")">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/font-awesome/css/font-awesome.min.css")">
    <!-- Ionicons -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/Ionicons/css/ionicons.min.css")">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/dist/css/AdminLTE.min.css")">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/dist/css/skins/_all-skins.min.css")">
    <!-- Morris chart -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/morris.js/morris.css")">
    <!-- jvectormap -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/jvectormap/jquery-jvectormap.css")">
    <!-- Date Picker -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css")">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/bower_components/bootstrap-daterangepicker/daterangepicker.css")">
    <link href="@Url.Content("~/adminLTE/plugins/bootstrap-dialog/bootstrap-dialog.min.css")" rel="stylesheet" />
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="@Url.Content("~/adminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css")">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
    <link href="@Url.Content("~/adminLTE/bower_components/select2/dist/css/select2.min.css")" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">

    <link href="~/Content/css/admin.min.css" rel="stylesheet" />
    <script>
        var isRequestAuthenticated='@User.Identity.IsAuthenticated.ToString().ToLower()';
    </script>
    @RenderSection("css", required: false)
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        @Html.Partial("~/Areas/Admin/Views/Shared/_HeaderPartial.cshtml");
        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("~/Areas/Admin/Views/Shared/_LeftSideMenuPartial.cshtml");

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.PageTitle
                    <small>@ViewBag.PageDesc </small>
                </h1>
                @*<ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li class="active">Dashboard</li>
                    </ol>*@
            </section>

            <!-- Main content -->
            @RenderBody()
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        @Html.Partial("~/Areas/Admin/Views/Shared/_FooterPartial.cshtml");
    </div>

    <div class="loading"></div>

    <!-- jQuery 3 -->
    <script src="@Url.Content("~/adminLTE/bower_components/jquery/dist/jquery.min.js")"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="@Url.Content("~/adminLTE/bower_components/jquery-ui/jquery-ui.min.js")"></script>
    <script src="@Url.Content("~/adminLTE/plugins/jqueryValidation/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/adminLTE/plugins/jqueryValidation/localization/messages_vi.min.js")"></script>
    <script src="@Url.Content("~/adminLTE/plugins/moment/moment.js")"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
    <script src="@Url.Content("~/adminLTE/plugins/select2/select2.full.min.js")"></script>

    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script src="@Url.Content("~/adminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/adminLTE/plugins/bootstrap-dialog/bootstrap-dialog.min.js")"></script>
    <!-- AdminLTE App -->
    <script src="https://cdn.jsdelivr.net/npm/lodash@4.17.11/lodash.min.js"></script>
    <script src="@Url.Content("~/adminLTE/dist/js/adminlte.min.js")"></script>
    <script src="@Url.Content("~/Scripts/common/helpers.js")"></script>
    <script>
        $(document).ajaxStart(function () {
            if (window.NotShowLoading == undefined || window.NotShowLoading == null || !window.NotShowLoading)
                $(".loading").fadeIn();
        });

        $(document).ajaxStop(function () {
            $(".loading").fadeOut();
            helpers.vndFormat();
        });

        $(document).ajaxComplete(function () {
            $(".loading").fadeOut();
            helpers.vndFormat();
        });

        $(document).ajaxError(function () {
            $(".loading").fadeOut();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
